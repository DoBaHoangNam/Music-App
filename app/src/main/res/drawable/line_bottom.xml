<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:bottom="0dp"
        android:left="0dp"
        android:right="0dp"
        android:top="0dp">
        <shape android:shape="rectangle">
            <stroke
                android:width="0.5dp"
                android:color="#FCFCFC" />
            <solid android:color="#00FFFFFF" />
            <stroke
                android:width="1dp"
                android:color="#FBF9F9"
                android:dashGap="0dp"
                android:dashWidth="5dp" />
            <padding
                android:bottom="5dp"
                android:left="5dp"
                android:right="5dp"
                android:top="5dp" />
        </shape>
    </item>
</layer-list>